    "@cubejs-backend/shared": "^0.25.32",
    "@hapi/joi": "^15.1.1",
    "codesandbox-import-utils": "^2.1.12",
    "cross-spawn": "^7.0.1",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "fs-extra": "^8.1.0",
    "http-proxy-agent": "^4.0.1",
    "is-docker": "^2.1.1",
    "jsonwebtoken": "^8.4.0",
    "lru-cache": "^5.1.1",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.0",
    "prettier": "^1.16.4",
    "promise-timeout": "^1.3.0",
    "ramda": "^0.27.0",
    "semver": "^6.3.0",
    "serve-static": "^1.13.2",
    "sqlstring": "^2.3.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@cubejs-backend/linter": "^0.24.5",
    "@types/cross-spawn": "^6.0.2",
    "@types/express": "^4.17.9",
    "@types/hapi__joi": "^15.0.4",
    "@types/jest": "^25.2.3",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^10.17.49",
    "@types/node-fetch": "^2.5.7",
    "@types/ramda": "^0.27.34",
    "@types/uuid": "^3.4.9",
    "jest": "^25.5.4",
    "typescript": "^4.1.3"
  },
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": "../cubejs-linter"
