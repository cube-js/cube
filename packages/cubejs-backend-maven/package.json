{
  "name": "@cubejs-backend/maven",
  "description": "Cube.js Maven Wrapper for java dependencies downloading",
  "author": "Cube Dev, Inc.",
  "version": "0.30.53",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.js.git",
    "directory": "packages/cubejs-druid-driver"
  },
  "engines": {
    "node": "^12.0.0 || ^14.0.0 || >=16.0.0"
  },
  "bin": {
    "maven-resolve": "bin/maven-resolve"
  },
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npm run tsc",
    "tsc": "tsc",
    "watch": "tsc -w",
    "unit": "jest dist/test/*.js",
    "lint": "eslint src/* --ext .ts",
    "lint:fix": "eslint --fix src/* --ext .ts"
  },
  "files": [
    "README.md",
    "bin",
    "dist/src/*"
  ],
  "dependencies": {
    "@cubejs-backend/shared": "^0.30.53",
    "source-map-support": "^0.5.19",
    "xmlbuilder2": "^2.4.0"
  },
  "devDependencies": {
    "@cubejs-backend/linter": "^0.30.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^12",
    "jest": "^26.6.3",
    "typescript": "~4.1.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "../cubejs-linter"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
