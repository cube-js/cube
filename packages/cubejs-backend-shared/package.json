{
  "name": "@cubejs-backend/shared",
  "version": "0.24.3",
  "description": "Shared code for Cube.js backend packages",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "watch": "tsc -w",
