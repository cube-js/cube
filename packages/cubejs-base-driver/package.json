    "@cubejs-backend/shared": "1.3.61",
    "@google-cloud/storage": "^7.13.0"
  },
  "devDependencies": {
    "@cubejs-backend/linter": "1.3.61",
    "@types/jest": "^29",
    "@types/node": "^20",
    "jest": "^29",
    "typescript": "~5.2.2"
