{
  "name": "@cubejs-backend/schema-compiler",
  "description": "Cube.js schema compiler",
  "author": "Cube Dev, Inc.",
  "version": "0.27.19",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.js.git",
    "directory": "packages/cubejs-schema-compiler"
  },
  "engines": {
    "node": ">=10.8.0"
  },
  "files": [
    "dist/src",
    "adapter",
    "scaffolding"
  ],
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npm run tsc",
    "generate": "antlr4ts -visitor src/parser/GenericSql.g4",
    "tsc": "tsc",
    "watch": "tsc -w",
    "test": "npm run unit && npm run integration",
    "unit": "TZ=UTC jest --coverage dist/test/unit",
    "integration": "TZ=UTC jest dist/test/integration/*",
    "integration:mssql": "TZ=UTC jest dist/test/integration/mssql",
    "integration:mysql": "TZ=UTC jest dist/test/integration/mysql",
    "integration:postgres": "TZ=UTC jest dist/test/integration/postgres",
    "integration:clickhouse": "TZ=UTC jest dist/test/integration/clickhouse",
    "lint": "eslint src/* test/* --ext .ts,.js",
    "lint:fix": "eslint --fix src/* test/* --ext .ts,.js"
  },
  "dependencies": {
    "@babel/code-frame": "^7.12.11",
    "@babel/core": "^7.12.10",
    "@babel/generator": "^7.12.10",
    "@babel/parser": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/standalone": "^7.12.10",
    "@babel/traverse": "^7.12.10",
    "@babel/types": "^7.12.12",
    "@cubejs-backend/shared": "^0.27.17",
    "@hapi/joi": "^15.1.1",
    "antlr4ts": "0.5.0-alpha.4",
    "camelcase": "^6.2.0",
    "cron-parser": "^2.16.3",
    "humps": "^2.0.1",
    "inflection": "^1.12.0",
    "lru-cache": "^5.1.1",
    "moment-range": "^4.0.1",
    "moment-timezone": "^0.5.33",
    "node-dijkstra": "^2.5.0",
    "ramda": "^0.27.0",
    "syntax-error": "^1.3.0"
  },
  "devDependencies": {
    "@apla/clickhouse": "^1.5.5",
    "@cubejs-backend/linter": "^0.27.0",
    "@types/babel__code-frame": "^7.0.2",
    "@types/babel__generator": "^7.6.2",
    "@types/babel__parser": "^7.1.1",
    "@types/babel__traverse": "^7.11.0",
    "@types/hapi__joi": "^17.1.0",
    "@types/inflection": "^1.5.28",
    "@types/jest": "^26.0.20",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^8.10.66",
    "@types/ramda": "^0.27.34",
    "@types/sqlstring": "^2.3.0",
    "@types/syntax-error": "^1.4.1",
    "@types/uuid": "^3.4.9",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "jest": "^26.6.3",
    "mssql": "^6.1.0",
    "mysql": "^2.18.1",
    "pg-promise": "^10.5.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "source-map-support": "^0.5.19",
    "sqlstring": "^2.3.1",
    "testcontainers": "^6.4.2",
    "typescript": "~4.2.4",
    "uuid": "^3.3.2"
  },
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": "../cubejs-linter"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": false,
    "coverageDirectory": "coverage/",
    "collectCoverageFrom": [
      "dist/src/**/*.js",
      "dist/src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      ".*\\.d\\.ts"
    ]
  }
}
