{
  "name": "@cubejs-backend/postgres-driver",
  "description": "Cube.js Postgres database driver",
  "author": "Cube Dev, Inc.",
  "version": "1.3.61",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.git",
    "directory": "packages/cubejs-postgres-driver"
  },
  "engines": {
    "node": "^14.0.0 || ^16.0.0 || >=17.0.0"
  },
  "files": [
    "dist/src",
    "index.js"
