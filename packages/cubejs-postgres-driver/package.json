    "@types/pg-query-stream": "^1.0.3",
    "moment": "^2.24.0",
    "pg": "^8.6.0",
    "pg-query-stream": "^4.1.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@cubejs-backend/linter": "1.3.61",
    "@cubejs-backend/testing-shared": "1.3.61",
    "testcontainers": "^10.28.0",
    "typescript": "~5.2.2"
  },
  "publishConfig": {
