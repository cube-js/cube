{
  "name": "@cubejs-backend/server",
  "description": "Cube.js all-in-one server",
  "author": "Cube Dev, Inc.",
  "version": "0.31.41",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.js.git",
    "directory": "packages/cubejs-server"
  },
  "engines": {
    "node": "^12.0.0 || ^14.0.0 || >=16.0.0"
  },
  "main": "index.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist/src",
    "dist/command",
    "bin",
    "index.js",
    "/oclif.manifest.json"
  ],
  "bin": {
    "cubejs-server": "./bin/server",
    "cubejs-dev-server": "./bin/dev-server"
  },
  "cubejsCliVersion": ">=0.22",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "OCLIF_TS_NODE=0 oclif-dev manifest",
    "build": "rm -rf dist && npm run tsc",
    "tsc": "tsc",
    "watch": "tsc -w",
    "test": "npm run unit",
    "lint": "eslint src/* test/ --ext .ts,.js",
    "lint:fix": "eslint --fix src/* test/ --ext .ts,js",
    "unit": "jest",
    "unit:debug": "jest --runInBand",
    "jest:shapshot": "jest --updateSnapshot test"
  },
  "dependencies": {
    "@cubejs-backend/cubestore-driver": "^0.31.41",
    "@cubejs-backend/dotenv": "^9.0.2",
    "@cubejs-backend/server-core": "^0.31.41",
    "@cubejs-backend/shared": "^0.31.41",
    "@oclif/color": "^1.0.0",
    "@oclif/command": "^1.8.13",
    "@oclif/config": "^1.18.2",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3.2.0",
    "@yarnpkg/lockfile": "^1.1.0",
    "body-parser": "^1.19.0",
    "codesandbox-import-utils": "^2.1.12",
    "cors": "^2.8.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.4.0",
    "semver": "^7.3.2",
    "source-map-support": "^0.5.19",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@cubejs-backend/linter": "^0.31.0",
    "@cubejs-backend/query-orchestrator": "^0.31.41",
    "@oclif/dev-cli": "^1.23.1",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.11",
    "@types/node": "^12",
    "@types/semver": "^7.3.4",
    "@types/ws": "^7.2.9",
    "@types/yarnpkg__lockfile": "^1.1.4",
    "jest": "^26.0.0",
    "typescript": "~4.1.5"
  },
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": "../cubejs-linter"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/dist/test/*.(test|spec).(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "oclif": {
    "commands": "./dist/src/command",
    "bin": "cubejs-server",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
