{
  "name": "@cubejs-backend/server",
  "description": "Cube.js all-in-one server",
  "author": "Cube Dev, Inc.",
  "version": "0.25.1",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.js.git",
    "directory": "packages/cubejs-server"
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "main": "index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "/oclif.manifest.json"
  ],
  "bin": {
    "cubejs-server": "./bin/server.js",
    "cubejs-dev-server": "./bin/dev-server.js"
  },
  "cubejsCliVersion": ">=0.22",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "npm run tsc && oclif-dev manifest",
    "build": "rm -rf dist && npm run tsc",
    "tsc": "tsc",
    "watch": "tsc -w",
    "test": "npm run unit",
    "lint": "linter src/* tests/ --ext .ts,.js",
    "lint:fix": "linter --fix src/* tests/ --ext .ts,js",
    "unit": "jest",
    "unit:debug": "jest --runInBand",
    "jest:shapshot": "jest --updateSnapshot"
  },
  "dependencies": {
    "@cubejs-backend/server-core": "^0.25.1",
    "@cubejs-backend/shared": "^0.25.0",
    "@oclif/color": "^0.1.2",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3.2.0",
    "@yarnpkg/lockfile": "^1.1.0",
    "body-parser": "^1.19.0",
    "codesandbox-import-utils": "^2.1.12",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.4.0",
    "node-machine-id": "^1.1.10",
    "semver": "^7.3.2",
    "source-map-support": "^0.5.19",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@cubejs-backend/linter": "^0.25.0",
    "@oclif/dev-cli": "^1.23.1",
    "@types/cors": "^2.8.8",
    "@types/node": "^10.0.10",
    "@types/semver": "^7.3.4",
    "@types/ws": "^7.2.9",
    "@types/yarnpkg__lockfile": "^1.1.4",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^5.2.1",
    "jest": "^26.0.0",
    "should": "^13.2.3",
    "ts-jest": "^26.0.0",
    "typescript": "~4.1.2"
  },
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": "../cubejs-linter"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/tests/*.(test|spec).(ts|js)"
    ]
  },
  "oclif": {
    "commands": "./dist/command",
    "bin": "cubejs-server",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
