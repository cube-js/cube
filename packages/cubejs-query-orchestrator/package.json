{
  "name": "@cubejs-backend/query-orchestrator",
  "description": "Cube.js Query Orchestrator and Cache",
  "author": "Cube Dev, Inc.",
  "version": "0.24.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.js.git",
    "directory": "packages/cubejs-query-orchestrator"
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npm run tsc",
    "tsc": "tsc",
    "watch": "tsc -w",
    "test": "npm run unit && npm run integration",
    "unit": "jest --runInBand --verbose test/unit",
    "integration": "npm run integration:redis",
    "integration:redis": "jest --runInBand --verbose test/integration",
    "lint": "./node_modules/@cubejs-backend/linter/node_modules/eslint/bin/eslint.js src/* test/* --ext .ts,.js",
    "lint:fix": "./node_modules/@cubejs-backend/linter/node_modules/eslint/bin/eslint.js --fix src/* test/* --ext .ts,.js"
  },
  "files": [
    "README.md",
    "dist/*"
  ],
  "dependencies": {
    "generic-pool": "^3.7.1",
    "ramda": "^0.27.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@cubejs-backend/linter": "^0.24.0",
    "@types/generic-pool": "^3.1.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^8.10.66",
    "@types/ramda": "^0.27.32",
    "@types/redis": "^2.8.28",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "license": "Apache-2.0",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/lib/"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "eslintConfig": {
    "extends": "../cubejs-linter"
  }
}
