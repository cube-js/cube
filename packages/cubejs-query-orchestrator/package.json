{
  "name": "@cubejs-backend/query-orchestrator",
  "description": "Cube.js Query Orchestrator and Cache",
  "author": "Cube Dev, Inc.",
  "version": "0.25.15",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.js.git",
    "directory": "packages/cubejs-query-orchestrator"
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npm run tsc",
    "tsc": "tsc",
    "watch": "tsc -w",
    "test": "npm run unit && npm run integration",
    "unit": "jest --runInBand --verbose dist/test/unit",
    "integration": "npm run integration:redis",
    "integration:redis": "jest --runInBand --verbose dist/test/integration",
    "integration:dynamodb": "jest --runInBand --verbose dist/test/integration/QueryQueueDynamoDB.test.js",
    "lint": "linter src/* test/* --ext .ts,.js",
    "lint:fix": "linter --fix src/* test/* --ext .ts,.js"
  },
  "files": [
    "README.md",
    "driver/*",
    "orchestrator/*",
    "dist/src/*"
  ],
  "dependencies": {
    "@cubejs-backend/shared": "^0.25.15",
    "aws-sdk": "^2.802.0",
    "dynamodb-toolbox": "^0.3.1",
    "generic-pool": "^3.7.1",
    "ramda": "^0.27.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@cubejs-backend/linter": "^0.25.0",
    "@types/generic-pool": "^3.1.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^8.10.66",
    "@types/ramda": "^0.27.32",
    "@types/redis": "^2.8.28",
    "jest": "^26.6.3",
    "testcontainers": "^6.4.1",
    "typescript": "~4.1.2"
  },
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": "../cubejs-linter"
  }
}
