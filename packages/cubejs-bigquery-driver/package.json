{
  "name": "@cubejs-backend/bigquery-driver",
  "description": "Cube.js BigQuery database driver",
  "author": "Cube Dev, Inc.",
  "version": "1.3.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.git",
    "directory": "packages/cubejs-bigquery-driver"
  },
  "engines": {
    "node": "^14.0.0 || ^16.0.0 || >=17.0.0"
  },
  "scripts": {
    "build": "rm -rf dist && npm run tsc",
    "tsc": "tsc",
    "watch": "tsc -w",
    "test": "yarn integration",
    "integration": "npm run integration:bigquery",
    "integration:bigquery": "jest --verbose dist/test",
    "lint": "eslint src/* --ext .ts",
    "lint:fix": "eslint --fix src/* --ext .ts"
  },
  "files": [
    "dist/src",
    "index.js"
  ],
  "main": "index.js",
  "types": "dist/src/index.d.ts",
  "dependencies": {
    "@cubejs-backend/base-driver": "1.3.8",
    "@cubejs-backend/dotenv": "^9.0.2",
    "@cubejs-backend/shared": "1.3.8",
    "@google-cloud/bigquery": "^7.7.0",
    "@google-cloud/storage": "^7.13.0",
    "ramda": "^0.27.2"
  },
  "devDependencies": {
    "@cubejs-backend/testing-shared": "1.3.8",
    "@types/big.js": "^6.2.2",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^29",
    "@types/ramda": "^0.27.40",
    "jest": "^29",
    "typescript": "~5.2.2"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "html",
      "lcov"
    ],
    "coverageDirectory": "coverage/",
    "collectCoverageFrom": [
      "dist/src/**/*.js",
      "dist/src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      ".*\\.d\\.ts"
    ],
    "moduleNameMapper": {
      "^uuid$": "<rootDir>/../../node_modules/uuid/dist/index.js",
      "^yaml$": "<rootDir>/../../node_modules/yaml/dist/index.js"
    }
  },
  "eslintConfig": {
    "extends": "../cubejs-linter"
  }
}
