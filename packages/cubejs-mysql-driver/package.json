{
  "name": "@cubejs-backend/mysql-driver",
  "description": "Cube.js Mysql database driver",
  "author": "Cube Dev, Inc.",
  "version": "1.2.16",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-js/cube.git",
    "directory": "packages/cubejs-mysql-driver"
  },
  "engines": {
    "node": "^14.0.0 || ^16.0.0 || >=17.0.0"
  },
  "files": [
    "dist/src",
    "index.js"
  ],
  "main": "index.js",
