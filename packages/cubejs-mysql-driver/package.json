    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@cubejs-backend/linter": "1.2.16",
    "@cubejs-backend/testing-shared": "1.2.16",
    "@types/generic-pool": "^3.1.9",
    "@types/jest": "^27",
    "jest": "^27",
    "stream-to-array": "^2.3.0",
    "testcontainers": "^10.13.0",
    "typescript": "~5.2.2"
  },
  "eslintConfig": {
    "extends": "../cubejs-linter"
