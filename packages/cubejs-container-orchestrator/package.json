{
  "name": "@cubejs-backend/container-orchestrator",
  "description": "Docker container orchestrator for @cubejs-backend/server-test",
  "author": "Statsbot, Inc.",
  "version": "0.1.0",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node scripts/withEnv.js 'nodemon --inspect=5858 src/index.js'",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty src __tests__",
    "lint:fix": "npm run --silent lint --fix; exit 0",
    "e2e": "node scripts/test.js --testPathPattern=__e2e__/.*\\.test\\.js",
    "e2e:debug": "node --inspect-brk scripts/test.js --testPathPattern=__e2e__/.*\\.test\\.js --runInBand",
    "test": "node scripts/test.js --testPathPattern=__tests__/.*\\.test\\.js",
    "test:debug": "node --inspect-brk scripts/test.js --testPathPattern=__tests__/.*\\.test\\.js"
  },
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "deepmerge": "^4.0.0",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "log-symbols": "^3.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "simple-git": "^1.123.0",
    "socket.io-client": "^2.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "node scripts/gitExecuteLocally.js -e \"npm run lint:fix && CI=1 npm run test && CI=1 npm run e2e\" --root \"../../\""
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "command-exists": "^1.2.8",
    "diskusage": "^1.1.3",
    "executable": "^4.1.1",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1",
    "socket.io": "^2.2.0",
    "which": "^1.3.1"
  }
}
