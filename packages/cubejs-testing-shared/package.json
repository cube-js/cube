{
  "name": "@cubejs-backend/testing-shared",
  "version": "1.3.8",
  "description": "Cube.js Testing Helpers",
  "author": "Cube Dev, Inc.",
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "engines": {
    "node": "^14.0.0 || ^16.0.0 || >=17.0.0"
  },
  "scripts": {
    "build": "rm -rf dist && npm run tsc",
    "tsc": "tsc",
    "watch": "tsc -w",
    "lint": "eslint src/* --ext .ts",
    "lint:fix": "eslint --fix src/* --ext .ts"
  },
  "files": [
    "dist/src/*"
  ],
  "dependencies": {
    "@cubejs-backend/dotenv": "^9.0.2",
    "@cubejs-backend/query-orchestrator": "1.3.8",
    "@cubejs-backend/schema-compiler": "1.3.8",
    "@cubejs-backend/shared": "1.3.8",
    "@testcontainers/kafka": "~10.13.0",
    "dedent": "^0.7.0",
    "node-fetch": "^2.6.7",
    "testcontainers": "^10.13.0"
  },
  "devDependencies": {
    "@cubejs-backend/linter": "1.3.8",
    "@jest/globals": "^29",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^29",
    "@types/node": "^20",
    "jest": "^29",
    "typescript": "~5.2.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      ".*\\.d\\.ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/dist/test/*.(test|spec).(ts|js)"
    ],
    "moduleNameMapper": {
      "^uuid$": "<rootDir>/../../node_modules/uuid/dist/index.js"
    },
    "snapshotResolver": "<rootDir>/test/snapshotResolver.js",
    "snapshotFormat": {
      "escapeString": true,
      "printBasicPrototype": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "../cubejs-linter"
  }
}
