{
  "cube": {
    "environment": {
      "CUBEJS_DEV_MODE": true,
      "CUBEJS_API_SECRET": "mysupersecret",
      "CUBEJS_CACHE_AND_QUEUE_DRIVER": "cubestore",
      "CUBEJS_CUBESTORE_HOST": "store",
      "CUBEJS_DB_TYPE": "clickhouse",
      "CUBEJS_DB_HOST": "data",
      "CUBEJS_DB_NAME": "test",
      "CUBEJS_DB_USER": "test",
      "CUBEJS_DB_PASS": "test"
    },
    "depends_on": ["data"],
    "links": ["data"],
    "volumes": [
      "./cube.js:/cube/conf/cube.js",
      "./schema/ecommerce.yaml:/cube/conf/schema/ecommerce.yaml"
    ],
    "ports" : ["4000"]
  },
  "data": {
    "image": "clickhouse/clickhouse-server",
    "environment": {
      "CLICKHOUSE_DB": "test",
      "CLICKHOUSE_USER": "test",
      "CLICKHOUSE_PASSWORD": "test",
      "CLICKHOUSE_DEFAULT_ACCESS_MANAGEMENT": 1
    },
    "ports" : ["8123"]
  },
  "cast": {
    "SELECT_PREFIX": "",
    "SELECT_SUFFIX": "",
    "DATE_PREFIX": "toDateTime(",
    "DATE_SUFFIX": ")",
    "CREATE_TBL_PREFIX": "CREATE TABLE ",
    "CREATE_TBL_SUFFIX": " ENGINE = Memory AS ",
    "CREATE_SUB_PREFIX": "",
    "CREATE_SUB_SUFFIX": ""
  },
  "preAggregations": {
    "Products": [],
    "Customers": [
      {
        "name": "Rolling",
        "measures": ["CUBE.count", "CUBE.runningTotal"],
        "indexes": {
          "category_index": {
            "columns": [
              "CUBE.count",
              "CUBE.runningTotal"
            ]
          }
        }
      }
    ],
    "ECommerce": [
      {
        "name": "SimpleAnalysis",
        "dimensions": ["CUBE.productName"],
        "measures": [
          "CUBE.totalQuantity",
          "CUBE.avgDiscount",
          "CUBE.totalSales",
          "CUBE.totalProfit"
        ],
        "indexes": {
          "category_index": {
            "columns": ["CUBE.productName"]
          }
        }
      },
      {
        "name": "TimeAnalysis",
        "time_dimension": "CUBE.orderDate",
        "granularity": "month",
        "partition_granularity": "month",
        "dimensions": ["CUBE.productName"],
        "measures": [
          "CUBE.totalQuantity",
          "CUBE.avgDiscount",
          "CUBE.totalSales",
          "CUBE.totalProfit"
        ],
        "indexes": {
          "category_index": {
            "columns": ["CUBE.productName"]
          }
        }
      }
    ]
  },
  "skip": [
