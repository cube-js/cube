{
  "name": "egraph-debug",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "elkjs": "^0.9.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "reactflow": "^11.10.3"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false && react-scripts start",
    "build": "react-scripts build",
    "reformat": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "prettier": "3.3.3",
    "react-scripts": "5.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
