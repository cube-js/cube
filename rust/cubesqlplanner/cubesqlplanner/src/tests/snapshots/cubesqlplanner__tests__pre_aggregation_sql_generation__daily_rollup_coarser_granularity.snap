SELECT "orders__country" "orders__country", date_trunc('month', "orders__created_at_day") "orders__created_at_month", sum("orders__count") "orders__count" 
FROM  orders__daily_countries_rollup  AS "orders__daily_countries_rollup"
GROUP BY 1, 2
ORDER BY  2  ASC
